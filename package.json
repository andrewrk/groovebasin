    }
  ],
  "engines": {
    "node": "~0.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/superjoe30/groovebasin.git"
  },
  "dependencies": {
    "socket.io": "~0.9.10",
    "lastfm": "~0.8.1",
    "zipstream": "~0.2.1",
    "express": "~3.0.0rc5",
    "temp": "~0.4.0",
    "naught": "~0.2.2",
    "async": "~0.1.22",
    "superagent": "~0.9.5",
    "mkdirp": "~0.3.4",
    "mv": "~0.0.3",
    "findit": "~0.1.2",
    "which": "~1.0.5",
    "osenv": "0.0.3",
    "mutagen": "~1.0.0",
    "walk": "~2.2.1"
  },
  "devDependencies": {
    "handlebars": "~1.0.7",
    "stylus": "~0.29.0",
    "node-dev": "~0.2.8",
    "jspackage": "~0.4.9",
    "watch": "~0.5.1",
    "coco": "~0.8.2"
  },
  "scripts": {
    "start": "naught start --ipc-file run/naught.ipc --log run/naught.log --stdout run/stdout.log --stderr run/stderr.log lib/server.js",
